{
  "name": "stability-image-to-video",
  "version": "1.0.0",
  "description": "A TypeScript script to use the Stability AI Image-to-Video API.",
  "main": "./dist/index.js",
  "scripts": {
    "build": "tsc -p tsconfig.json",
    "start": "node dist/index.js",
    "prestart": "npm run build",
    "kill": "lsof -ti :3000 | xargs kill -9",
    "deploy": "npm run build && fly deploy",
    "dev": "npm run kill && ts-node src/server.ts",
    "dev:server": "ts-node-dev --respawn src/server.ts",
    "download": "ts-node src/scripts/download-video.ts"
  },
  "keywords": [
    "stability-ai",
    "image-to-video",
    "typescript"
  ],
  "author": "",
  "license": "ISC",
  "dependencies": {
    "@aws-sdk/client-ses": "^3.840.0",
    "@aws-sdk/client-sesv2": "^3.840.0",
    "@types/cors": "^2.8.17",
    "@types/express": "^4.17.21",
    "@types/node": "^20.19.4",
    "@types/sharp": "^0.31.1",
    "@types/stripe": "^8.0.416",
    "axios": "^1.10.0",
    "cookie-parser": "^1.4.7",
    "cors": "^2.8.5",
    "dotenv": "^16.4.5",
    "express": "^4.18.2",
    "form-data": "^4.0.0",
    "jsonwebtoken": "^9.0.2",
    "multer": "^2.0.1",
    "nodemailer": "^7.0.4",
    "sharp": "^0.34.2",
    "stripe": "^18.3.0"
  },
  "devDependencies": {
    "@flydotio/dockerfile": "^0.7.10",
    "@types/cookie-parser": "^1.4.9",
    "@types/jsonwebtoken": "^9.0.10",
    "@types/multer": "^2.0.0",
    "@types/nodemailer": "^6.4.17",
    "ts-node": "^10.9.2",
    "ts-node-dev": "^2.0.0",
    "typescript": "^5.4.5"
  }
}
